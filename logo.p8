pico-8 cartridge // http://www.pico-8.com
version 35
__lua__
--display logo
i=0
k=0
j=0
function logo()
	if i<8 then
	 spr(1, 32, 32, 8, i)
	else
		spr(1, 32, 32, 8, 8)
	end
	if k>2 then
		cls() 
		spr(1, 32, 40, 8, 8)
	end
	if j>2 then
		cls() 
		spr(1, 32, 32, 8, 8)
		print("start?", 44, 90, 11)
		print("press x", 42, 96, 11)
	end	
end

function _update()
	i+=0.4
	if i>8 then
		k+=.15
	end
	if k>2 then
		j+=.15
	end 
end

function _draw()
	
	cls()
	logo()
	
end
